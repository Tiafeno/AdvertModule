		{
			"id": "Furnished and Unfurnished",
			"type": "select",
			"validate": "furnished"
		},
		{
			"id": "Mark",
			"type": "select",
			"validate": "mark"
		},
		{
			"id": "Model",
			"type": "text",
			"validate": "model",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser le Modèle.."
				}
			}
		},
		{
			"id": "Model Year",
			"type": "date",
			"validate": "regdate",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez choisir l'année-modèle."
				}
			}
		},
		{
			"id": "Fuel",
			"type": "select",
			"validate": "fuel",
			"options": [
				{
					"_id": 1,
					"name": "Gasoline"
				},
				{
					"_id": 2,
					"name": "Diesel"
				},
				{
					"_id": 3,
					"name": "GPL"
				},
				{
					"_id": 4,
					"name": "Electric"
				},
				{
					"_id": 5,
					"name": "Hybrid"
				},
				{
					"_id": 6,
					"name": "Other"
				}
			],
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez choisir le carburant."
				}
			}
		},
		{
			"id": "Gearbox",
			"type": "select",
			"validate": "gearbox",
			"options":[
				{
					"_id": 1,
					"name": "Manual"
				},
				{
					"_id": 2,
					"name": "Automatic"
				}
			],
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez choisir la boîte de vitesse."
				}
			}
		},
		{
			"id": "Mielage",
			"type": "text",
			"validate": "mileage",
			"placeholder": "km",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser le kilométrage."
				}
			}
		},
		{
			"id": "Displacement",
			"type": "text",
			"validate": "cubic_capacity",
			"placeholder": "cm3",
			"scope": {
				"required": false
			}
		},
		{
			"id": "Swimming pool",
			"type": "select",
			"valiate": "swimming",
			"options" : [
				{
					"_id": 1,
					"name": "Yes"
				},
				{
					"_id": 2,
					"name": "No"
				}
			]
		}
