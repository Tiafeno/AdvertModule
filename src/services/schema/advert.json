	"vendor": [
		{
			"id": "Area",
			"type": "number",
			"validate": "area",
			"scope": {
				"maxlength": 6,
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser la surface.."
				}
			}
		},
		{
			"id": "Kind of good",
			"type": "select",
			"validate": "real_estate_type",
			"options": [
				{
					"name": "House",
					"_id": 1
				},
				{
					"name": "Apartment",
					"_id": 2
				},
				{
					"name": "Car park",
					"_id": 3
				},
				{
					"name": "Ground",
					"_id": 4
				},
				{
					"name": "Other",
					"_id": 5
				}
			],
			"scope": {
				"required": false
			}
		},
		{
			"id": "Rooms",
			"type": "tel",
			"validate": "rooms",
			"scope": {
				"maxlength": 2,
				"ng_message": {
					"maxlength": "L'indication du nombre de pièces ne doit comporter que des caractères numériques."
				}
			}
		},
		{
			"id": "Furnished and Unfurnished",
			"type": "select",
			"validate": "furnished",
			"options": [
				{
					"_id": 1,
					"name": "Furnished"
				},
				{
					"_id": 2,
					"name": "Unfurnished"
				}
			]
		},
		{
			"id": "Mark",
			"type": "text",
			"validate": "mark",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser la Marque."
				}
			}
		},
		{
			"id": "Model",
			"type": "text",
			"validate": "model",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser le Modèle."
				}
			}
		},
		{
			"id": "Model Year",
			"type": "date",
			"datepicker": [
				{
					"key": "data-min-view",
					"value": "years"
				},
				{
					"key": "data-view",
					"value": "years"
				},
				{
					"key": "data-date-format",
					"value": "yyyy"
				}
			],
			"validate": "regdate",
			"scope": {}
		},
		{
			"id": "Fuel",
			"type": "select",
			"validate": "fuel",
			"options": [
				{
					"_id": 1,
					"name": "Gasoline"
				},
				{
					"_id": 2,
					"name": "Diesel"
				},
				{
					"_id": 3,
					"name": "GPL"
				},
				{
					"_id": 4,
					"name": "Electric"
				},
				{
					"_id": 5,
					"name": "Hybrid"
				},
				{
					"_id": 6,
					"name": "Other"
				}
			],
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez choisir le carburant."
				}
			}
		},
		{
			"id": "Gearbox",
			"type": "select",
			"validate": "gearbox",
			"options": [
				{
					"_id": 1,
					"name": "Manual"
				},
				{
					"_id": 2,
					"name": "Automatic"
				}
			],
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez choisir la boîte de vitesse."
				}
			}
		},
		{
			"id": "Mielage",
			"type": "text",
			"validate": "mileage",
			"placeholder": "km",
			"scope": {
				"required": true,
				"ng_message": {
					"required": "Veuillez préciser le kilométrage."
				}
			}
		},
		{
			"id": "Displacement",
			"type": "text",
			"validate": "cubic_capacity",
			"placeholder": "cm3",
			"scope": {
				"required": false
			}
		},
		{
			"id": "Swimming pool",
			"type": "select",
			"validate": "swimming",
			"options": [
				{
					"_id": 1,
					"name": "Yes"
				},
				{
					"_id": 2,
					"name": "No"
				}
			]
		},
		{
			"id": "Type Clothing",
			"type": "select",
			"validate": "type_clothing",
			"options": [
				{
					"_id": 1,
					"name": "Wife"
				},
				{
					"_id": 2,
					"name": "Man"
				},
				{
					"_id": 3,
					"name": "Pregnant woman"
				},
				{
					"_id": 4,
					"name": "Kids"
				}
			]
		},
		{
			"id": "Shoe size",
			"type": "sequence",
			"validate": "shoe_size",
			"sequences": {
				"min": 10,
				"max": 50
			}
		}
	]
